    "@angular/platform-browser": "~2.1.1",
    "@angular/platform-browser-dynamic": "~2.1.1",
    "@angular/router": "~3.1.1",
    "@angular/upgrade": "~2.1.1",
    "@types/bootstrap-slider": "^4.8.32",
    "@types/faker": "^3.1.32",
    "@types/googlemaps": "^3.25.36",
    "@types/icheck": "^0.8.28",
    "@types/jquery": "^2.0.33",
    "@types/owlcarousel": "0.0.28",
    "@types/selectize": "^0.12.28",
    "angular-in-memory-web-api": "~0.1.13",
    "angular2-notifications": "^0.4.46",
    "bootstrap": "^3.3.7",
    "bootstrap-slider": "^9.4.0",
    "core-js": "^2.4.1",
    "faker": "^3.1.0",
    "googlemaps": "^1.11.1",
    "icheck": "^1.0.2",
